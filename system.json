{
  "id": "tencandles",
  "title": "10 Candles",
  "description": "A system for playing the tragic horror game 10 Candles",
  "version": "1.0.0",
  "compatibility": {
    "minimum": "10",
    "verified": "11",
    "maximum": "11"
  },
  "scripts":[
    "./scripts/js.cookie.js"
  ],
  "esmodules":[
    "./tencandles.mjs",
    "./scripts/tenc-truths-app.mjs",
    "./scripts/tenc-player-hud.mjs",
    "./scripts/tenc-roller.mjs"
  ],
  "styles": [
    "styles/style.css"
  ],
  "socket": true,
  "relationships": {
    "requires": [{
      "id": "ten-candles",
      "type": "module",
      "manifest": "https://raw.githubusercontent.com/pints1ze/ten-candles/main/module.json",
      "compatibility": {
        "verified": "1.0.1"
      }
    },
    {
      "id": "socketlib",
      "type": "module",
      "manifest": "https://raw.githubusercontent.com/manuelVo/foundryvtt-socketlib/master/module.json",
      "compatibility": {
        "minimum": "10",
        "verified": "11"
      }
    }
  ]
  }
}